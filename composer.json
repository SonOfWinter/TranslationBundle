{
    "name": "sonofwinter/translation-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "This Bundle provides a translator from Entity",
    "keywords": [
        "symfony",
        "php",
        "bundle",
        "translation"
    ],
    "authors": [
        {
            "name": "Thomas Leduc",
            "email": "thomaslmoi15@hotmail.fr",
            "role": "Developer"
        }
    ],
    "autoload":{
        "psr-4":{
            "SOW\\TranslationBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": ">=5.0",
        "symfony/orm-pack": "*",
        "symfony/dependency-injection": ">=5.0",
        "symfony/yaml": ">=5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "symfony/phpunit-bridge": "^4.3 || ^5.0"
    }
}
